{
  "name": "nuxt-starter-backend",
  "version": "1.0.0",
  "description": "Backend API for Nuxt Starter App with Fastify",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "format": "prettier --write \"src/**/*.{ts,json}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/swagger": "^9.5.2",
    "@fastify/swagger-ui": "^5.2.3",
    "better-sqlite3": "^12.4.1",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "fastify": "^5.6.1",
    "fastify-type-provider-zod": "^6.0.0",
    "pg": "^8.16.3",
    "pino-pretty": "^13.1.1",
    "zod": "^4.1.11",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^24.6.1",
    "@types/pg": "^8.15.5",
    "@vitest/coverage-v8": "^3.2.4",
    "drizzle-kit": "^0.31.5",
    "prettier": "^3.6.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4"
  }
}
